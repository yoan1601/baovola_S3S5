<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <display-name>Servlet Application</display-name>
    <!-- class 1 -->
    <servlet>
        <servlet-name>GoToListMenuANDGoInsertForm</servlet-name>
        <servlet-class>mvc.PlatServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>GoToListMenuANDGoInsertForm</servlet-name>
        <url-pattern>/1</url-pattern>
    </servlet-mapping>
    <!-- class 1 -->
    <servlet>
        <servlet-name>GoToForm</servlet-name>
        <servlet-class>mvc.Form</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>GoToForm</servlet-name>
        <url-pattern>/formMenu</url-pattern>
    </servlet-mapping>
    <!-- class 1 -->
    <servlet>
        <servlet-name>Delete</servlet-name>
        <servlet-class>mvc.DeleteMenu</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Delete</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>
    
    
    
</web-app>